<?xml version="1.0" standalone='no'?>
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
  <!-- %h expands to the hostname -->
  <name replace-wildcards="yes">%h CUPS</name>

  <!-- Generic IPP service; CUPS will attach perâ€‘queue records -->
  <service>
    <type>_ipp._tcp</type>
    <subtype>_universal._sub._ipp._tcp</subtype>
    <port>631</port>
  </service>

  <!-- Legacy printer service for older clients -->
  <service>
    <type>_printer._tcp</type>
    <port>631</port>
  </service>
</service-group>
